 


 <% layout('layouts/boilerplate') %>
  <body>
    <h1>All Listings</h1>
    <form method="GET" action="/listings/new">
      <button type="submit">Add Listings</button>
    </form>

    <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
      <% for (let listing of allListings) { %>
        <a class="text-decoration-none" href="/listings/<%=listing._id%>">
        <div class="card col border-0 mb-2" style="width: 20rem;">
          <img class="rounded" src="<%= listing.image%> " class="card-img-top" alt="...">
          <div class="card-body">
            <p class="card-text "><b><%= listing.title %> </b><br>
              â‚¹ <%= listing.price%>/Night
            </p>
          </div>
        </div>
      </a>
        <% } %>

    </div>

   



    <ul>
      <% for (let listing of allListings) { %>

      <li>
        <a href="/listings/ <%= listing._id %>"> <%= listing.title %> </a>
      </li>
      <% } %>
    </ul>
  </body>
